{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "redo",
			"target": "all",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": "$rustc",
			"label": "redo: all"
		},
		{
			"type": "redo",
			"target": "test",
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"problemMatcher": "$rustc",
			"label": "redo: test"
		},
		{
			"type": "redo",
			"target": "check",
			"group": "build",
			"problemMatcher": "$rustc",
			"label": "redo: check"
		}
	]
}
